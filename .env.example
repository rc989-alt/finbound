# FinBound Environment Configuration
# Copy this file to .env and fill in your values

# ============================================================
# OPTION 1: OpenAI (Standard)
# ============================================================
# OPENAI_API_KEY=sk-your-api-key-here

# ============================================================
# OPTION 2: Azure OpenAI
# ============================================================
# Set these variables to use Azure OpenAI instead of OpenAI
AZURE_OPENAI_API_KEY=your-azure-api-key-here
AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com
AZURE_OPENAI_API_VERSION=2024-02-15-preview

# Azure deployment names (must match your Azure deployments)
AZURE_OPENAI_DEPLOYMENT_GPT4O=gpt-4o
AZURE_OPENAI_DEPLOYMENT_GPT4O_MINI=gpt-4o-mini

# ============================================================
# Performance Settings
# ============================================================

# Enable ultra-low latency mode (uses gpt-4o-mini, skips verification)
# FINBOUND_ULTRA_LOW_LATENCY=1

# Enable parallel verification passes
# FINBOUND_PARALLEL_VERIFICATION=1
